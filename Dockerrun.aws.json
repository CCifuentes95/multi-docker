{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions": [
        {
            "name":"client",
            "image": "ccifuentes95/multi-client",
            "hostname":"client",
            "essential": false,
            "memory": 256
        },
        {
            "name":"api",
            "image": "ccifuentes95/multi-api",
            "hostname":"api",
            "essential": false,
            "memory": 256
        },
        {
            "name":"worker",
            "image": "ccifuentes95/multi-worker",
            "hostname":"worker",
            "essential": false,
            "memory": 256
        },
        {
            "name":"server",
            "image": "ccifuentes95/multi-server",
            "hostname":"server",
            "essential": true,
            "portMappings": [
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["client","api"],
            "memory": 128
        }
    ]
}